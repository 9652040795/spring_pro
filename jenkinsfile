
pipeline {
  agent any
  tools { 
        maven 'maven3.6'
        jdk 'JAVA_HOME'
  }
  stages {
   stage('Clone repository') {
      steps {
        checkout scm
        echo "Sudhakar"
      }
    }
    stage('Build'){
      steps {
        sh 'mvn compile'
        echo "compilation done"
      }
    }
  }
}
