pipeline {
  agent any
  environment {
        MYJOB = 'DevOps1'
        BUILD = 'PIPELINE1'
  }
  stages {
   stage('Clone repository') {
      steps {
        checkout scm
        echo "Sudhakar"
        echo "$MYJOB"
        echo "$BUILD"
      }
    }
    stage('Build'){
      environment {
        MYJOB = 'DevOps2'
        BUILD = 'PIPELINE2'
    }
      steps {
        sh 'mvn test'
        junit "**/target/surefire-reports/*.xml"
        echo "compilation done"
        echo "$MYJOB"
        echo "$WORKSPACE"

      }
       }
  }
}
